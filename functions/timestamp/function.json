{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "timestampDatabase",
      "type": "cosmosDB",
      "databaseName": "DocumentStamp",
      "collectionName": "Timestamps",
      "createIfNotExists": true,
      "connectionStringSetting": "CosmosDbConnectionString",
      "direction": "out"
  }
  ]
}
